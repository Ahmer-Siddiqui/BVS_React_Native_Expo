{
  "expo": {
    "name": "Blockchain Voting System",
    "slug": "blockchain-voting-system",
    "scheme": "blockchain-voting-system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
